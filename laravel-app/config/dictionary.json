{
    "validation_rules": {
        "combinations": {
            "allowed": [
                {
                    "category": "taste_profiles",
                    "compatible_with": ["ingredients", "cooking_methods"]
                },
                {
                    "category": "textures",
                    "compatible_with": ["ingredients"]
                }
            ],
            "forbidden": [
                {
                    "category": "taste_profiles",
                    "terms": ["濃厚", "こってり"],
                    "not_compatible_with": ["さっぱり", "あっさり"]
                }
            ]
        },
        "required_fields": {
            "all_entries": ["base"],
            "ingredients": ["modifiers"],
            "cooking_methods": ["forms"]
        }
    },
